    "cookie": "0.4.0",
    "core-js": "3.6.5",
    "formdata-polyfill": "3.0.19",
    "js-cookie": "2.2.1",
    "numeral": "2.0.6",
    "uuid-validate": "0.0.3",
    "vue": "2.6.10",
    "vue-click-outside": "1.0.7",
    "vue-clipboard2": "0.2.1",
    "vue-i18n": "8.14.0",
    "vue-infinite-scroll": "2.0.2",
    "vue-inject": "2.1.1",
    "vue-masonry-css": "1.0.3",
